	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceFolder}\\breeze-export\\app.ts",
			"args": [
				"--site=kenwood",
				"--username=ddavenport",
				"--password=${input:password}"
			],
			"sourceMaps": true,
			"preLaunchTask": "npm: watch",
			"outFiles": [
				"${workspaceFolder}/breeze-export/out/**/*.js"
			]
		}
	],
	"inputs": [
		{
			"id": "password",
			"type": "promptString",
			"description": "password"
		}
	]
}